{
  "name": "app-frontend",
  "version": "0.0.21",
  "private": true,
  "main": "dist",
  "scripts": {
    "postinstall": "curl -o node_modules/parcel-bundler/src/transforms/babel.js https://gist.githubusercontent.com/unshift/cbc718c171210de92a6cbada86f479ab/raw/bde06df909468faa592f6cc4429b256bff520314/babel.js",
    "clean": "rimraf .cache && rimraf .parlay && rimraf dist && rimraf lib",
    "dev": "node bin/dev.js",
    "watch": "babel src -d lib --copy-files --watch",
    "prepare": "babel src -d lib --copy-files",
    "start": "pushstate-server dist",
    "build": "NODE_ENV=production node bin/build.js",
    "lint": "standard \"src/**/*.js\" --verbose --fix | snazzy",
    "deploy": "parlay deploy"
  },
  "dependencies": {
    "@material-ui/core": "^1.1.0",
    "@material-ui/icons": "^1.1.0",
    "apollo-cache-inmemory": "^1.2.0",
    "apollo-client": "^2.3.0",
    "apollo-link": "^1.2.0",
    "apollo-link-http": "^1.5.4",
    "apollo-link-ws": "^1.0.6",
    "apollo-utilities": "^1.0.8",
    "aws-sdk": "^2.218.1",
    "date-fns": "^1.29.0",
    "draft-js": "^0.10.5",
    "draft-js-compact": "0.0.4",
    "graphql": "^0.13.2",
    "graphql-request": "^1.5.1",
    "graphql-tag": "^2.9.2",
    "haikunator": "^2.0.0",
    "immutable": "~3.8.2",
    "immutablepatch": "^0.4.2",
    "jsondiffpatch": "^0.3.10",
    "lambda-edge-router": "^0.0.16",
    "lodash.debounce": "^4.0.8",
    "lodash.escape": "^4.0.1",
    "lodash.isequal": "^4.5.0",
    "lodash.memoize": "^4.1.2",
    "lodash.unescape": "^4.0.1",
    "material-ui-icons": "^1.0.0-beta.36",
    "monograph": "^0.0.22",
    "node-sass": "^4.9.0",
    "prop-types": "^15.5.10",
    "query-string": "^6.0.0",
    "react": "16.4.0",
    "react-apollo": "^2.1.4",
    "react-dates": "^16.6.1",
    "react-dom": "16.4.0",
    "react-inspector": "^2.3.0",
    "react-keydown": "^1.7.4",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.1.2",
    "recompose": "^0.27.0",
    "redux": "^4.0.0",
    "simple-cache-provider": "^0.6.0",
    "subscriptions-transport-ws": "^0.9.7",
    "with-async-component": "^0.0.4"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "node_modules",
      "dist",
      ".cache",
      "src/__test__",
      "src/__store__"
    ],
    "plugins": [
      "flowtype"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.47",
    "@babel/core": "^7.0.0-beta.47",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.47",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.47",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0-beta.47",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.47",
    "@babel/plugin-transform-flow-strip-types": "^7.0.0-beta.47",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.47",
    "@babel/preset-env": "^7.0.0-beta.47",
    "@babel/preset-flow": "^7.0.0-beta.47",
    "@babel/preset-react": "^7.0.0-beta.47",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "execa": "^0.10.0",
    "flow-bin": "^0.73.0",
    "flow-remove-types": "^1.2.3",
    "immutablediff": "^0.4.4",
    "jest": "^23.0.1",
    "nightmare": "^3.0.1",
    "parcel-bundler": "^1.8.1",
    "parlay": "^0.0.121",
    "parlay-namecheap-plugin": "^0.0.121",
    "pushstate-server": "^3.0.1",
    "react-addons-perf": "^15.4.2",
    "react-test-renderer": "^16.3.2",
    "sinon": "^5.0.7",
    "snazzy": "^7.0.0",
    "standard": "^11.0.0-beta.0"
  }
}
